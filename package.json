{
  "name": "plataforma-jzf",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node -P server/tsconfig.json server/index.ts",
    "start": "npx prisma db push && node dist/server/prisma/seed.js && node dist/server/index.js",
    "build": "npm run build:ui && npm run build:server",
    "build:ui": "npx vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "npx vite preview",
    "postinstall": "npx prisma generate"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^0.14.0",
    "@prisma/client": "5.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.8.2",
    "marked": "12.0.2",
    "pdf-lib": "1.17.1",
    "prisma": "5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5",
    "vite": "5.2.12"
  },
  "devDependencies": {
    "autoprefixer": "10.4.19",
    "tailwindcss": "3.4.3"
  },
  "prisma": {
    "schema": "server/prisma/schema.prisma",
    "seed": "ts-node --compiler-options '{\\\"module\\\":\\\"commonjs\\\"}' server/prisma/seed.ts"
  }
}